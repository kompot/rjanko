{
  "name": "rjanko",
  "version": "0.0.1",
  "description": "ES6+ web framework for perfectionists with deadlines",
  "main": "index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "copyTemplates": "cp -r src/templates/ lib/templates/",
    "prepublish": "npm run compile; npm run copyTemplates",
    "test": "mocha --compilers js:babel/register",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover $npm_package_config_ISPARTA_OPTS ./node_modules/.bin/_mocha",
    "sendToCoveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "posttest": "npm run coverage && npm run sendToCoveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjanko/rjanko.git"
  },
  "bin": {
    "rjanko": "./lib/rjanko.js"
  },
  "author": "Anton Fedchenko <kompotik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjanko/rjanko/issues"
  },
  "homepage": "https://github.com/rjanko/rjanko",
  "dependencies": {
    "bluebird": "^2.9.24",
    "commander": "^2.7.1",
    "debug": "^2.1.3",
    "lodash": "^3.6.0",
    "mkdirp": "^0.5.0",
    "recursive-readdir": "^1.2.1"
  },
  "devDependencies": {
    "babel": "^5.0.12",
    "coveralls": "^2.11.2",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "rimraf": "^2.3.2"
  }
}
