{
  "name": "rjanko",
  "version": "0.0.2",
  "description": "ES6+ web framework for perfectionists with deadlines",
  "main": "index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "DEBUG=rjanko:* NODE_ENV=test mocha --compilers js:babel/register --timeout 180000",
    "lint": "eslint src",
    "posttest": "npm run lint",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover $npm_package_config_ISPARTA_OPTS ./node_modules/.bin/_mocha",
    "sendToCoveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kompot/rjanko.git"
  },
  "bin": {
    "rjanko": "./lib/rjanko.js"
  },
  "author": "Anton Fedchenko <kompotik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kompot/rjanko/issues"
  },
  "homepage": "https://github.com/kompot/rjanko",
  "dependencies": {
    "babel": "^5.1.10",
    "bluebird": "^2.9.24",
    "commander": "^2.8.0",
    "debug": "^2.1.3",
    "lodash": "^3.7.0",
    "mkdirp": "^0.5.0",
    "recursive-readdir": "^1.2.1"
  },
  "devDependencies": {
    "axios": "^0.5.4",
    "babel-eslint": "^3.0.1",
    "coveralls": "^2.11.2",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.1.1",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "rimraf": "^2.3.2"
  }
}
